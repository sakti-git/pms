<div class="row">

    <% include ../../partials/sidebar %>

    <form action="" id="form-filter" method="POST" style="padding-inline: 50px;">
        <div class="card">
            <div class="card-header">
                <h2>Add Member</h2>
            </div>

            <div class="card-body">

                <div class="row">
                    <div class="col" style="padding-left: 100px;">
                        <div class="form-check">
                            <label class="form-check-label" for="addMemberName">
                                Name
                            </label>
                        </div>
                    </div>
                    <div class="col-10" style="padding-right: 200px;">
                        <div class="form-group">
                            <input type="text" name="addMemberName" class="form-control" id="addMemberName"
                                placeholder="Member Name">
                        </div>
                    </div>
                </div>

                <div class="row">
                    <div class="col" style="padding-left: 100px;">
                        <div class="form-check">
                            <label class="form-check-label" for="addMemberPosition">
                                Position
                            </label>
                        </div>
                    </div>

                    <div class="col-10">

                        <div class="form-check">
                            <input type="checkbox" class="form-check-input" name="members" id="" value="">
                            <label class="form-check-label" for="members">

                            </label>
                        </div>

                    </div>
                </div>

                <div class="col" style="padding-left: 100px; padding-top: 10px;">
                    <button type="submit" class="btn btn-primary">Save</button>
                    <a href="" <button class="btn btn-warning">Back</button></a>
                </div>

            </div>

        </div>
    </form>
</div>

