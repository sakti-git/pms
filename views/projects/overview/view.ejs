</form>

<div class="row">

    <% include ../../partials/sidebar %>

    <form action="" id="form-filter" method="GET" style="padding-inline: 50px;" class="col-10">
        <div class="card">
            <div class="card-header">
                <h1>Overview</h1>
                <hr>
                <h3>Library System</h3>

                <div class="card">
                    <div class="card-body">
                        Issue Tracking :
                        <ul>
                            <li>Bug: <%= bugOpen %> Open/<%= bugTotal %></li>
                            <li>Feature: <%= featureOpen %> Open/<%= featureTotal %></li>
                            <li>Support: <%= supportOpen %> Open/<%= supportTotal %></li>
                        </ul>
                    </div>
                </div>

                <br>

                <div class="card">
                    <div class="card-body">
                        Members :
                        <ul>
                            <% members.forEach((item) => {%>
                            <li><%= item.firstname %> <%= item.lastname %></li>
                            <% }) %>
                        </ul>
                    </div>
                </div>

            </div>
        </div>
    </form>

</div>

<% include ../../partials/footer %>